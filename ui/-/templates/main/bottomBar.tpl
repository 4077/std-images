<div class="{__NODE_ID__}" instance="{__INSTANCE__}">

    <div class="left">
        <div class="copy button {COPY_BUTTON_DISABLED_CLASS}">Копировать</div>
        <div class="paste button {PASTE_BUTTON_DISABLED_CLASS}">Вставить <span class="count">({BUFFER_COUNT})</span></div>
        <div class="delete button {DELETE_BUTTON_DISABLED_CLASS}">Удалить</div>
    </div>

    <div class="right">
        <div class="select_all button {SELECT_ALL_BUTTON_DISABLED_CLASS}">выделить все</div>
        <div class="deselect button {DESELECT_BUTTON_DISABLED_CLASS}">снять выделение</div>
    </div>

</div>
